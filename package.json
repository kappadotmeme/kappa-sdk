{
  "name": "kappa-sdk",
  "version": "1.3.5",
  "description": "SDK for creating and trading tokens on Kappa's Sui bonding curve (create/buy/sell)",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./server": {
      "require": "./src/server.js",
      "import": "./src/server.js"
    },
    "./react": {
      "require": "./src/react/index.js",
      "import": "./src/react/index.js",
      "types": "./src/react/index.d.ts"
    }
  },
  "files": [
    "src",
    "kappa.js",
    "math.js",
    "move-bytecode"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {
    "@mysten/bcs": "^1.6.4",
    "@mysten/sui": "^1.36.0",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.45.1",
    "@mysten/dapp-kit": "^0.17.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "eslint": "^8.57.0"
  },
  "scripts": {
    "start": "node -e \"console.log('@kappa/sdk: library only')\"",
    "lint": "eslint .",
    "test": "node --test",
    "prepublishOnly": "npm run lint && npm test"
  },
  "keywords": [
    "sui",
    "blockchain",
    "token",
    "bonding-curve",
    "kappa",
    "sdk"
  ],
  "author": "Kappa Protocol",
  "license": "MIT"
}
